#include <stdio.h>

int main() {
    int n;
    printf("Enter n: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d numbers between 0 and %d (excluding one):\n", n, n);

    long long sum = 0;
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    long long total = (long long)n * (n + 1) / 2;
    int missing = (int)(total - sum);

    printf("The missing number is: %d\n", missing);

    return 0;
}
