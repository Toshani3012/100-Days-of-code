// Q16: Write a program to input three numbers and display the greatest among them.

#include <stdio.h>

int main() {
    int a, b, c;
    printf("Enter number 1: ");
    scanf("%d", &a);
    printf("Enter number 2: ");
    scanf("%d", &b);
    printf("Enter number 3: ");
    scanf("%d", &c);

    if (a >= b && a >= c)
        printf("%d is the greatest.\n", a);
    else if (b >= a && b >= c)
        printf("%d is the greatest.\n", b);
    else
        printf("%d is the greatest.\n", c);

    return 0;
}
