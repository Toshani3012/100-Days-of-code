#include <stdio.h>

int main() {
    int a, b;

    printf("Enter number 1: ");
    scanf("%d", &a);

    printf("Enter number 2: ");
    scanf("%d", &b);

    // Swapping without a third variable
    a = a + b;
    b = a - b;
    a = a - b;

    printf("Swapped numbers are: %d, %d\n", a, b);

    return 0;
}
