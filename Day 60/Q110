// Q110 (Logic Enhancers)
// Program to find the maximum in each subarray of size k (brute-force)

#include <stdio.h>

int main() {
    int arr[100], size, k;

    // Input array size and elements
    printf("Enter size of array: ");
    scanf("%d", &size);
    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    // Input window size k
    printf("Enter window size k: ");
    scanf("%d", &k);

    // Edge case check
    if (k > size || k <= 0) {
        printf("Invalid window size\n");
        return 1;
    }

    // Brute-force sliding window
    for (int i = 0; i <= size - k; i++) {
        int max = arr[i];
        for (int j = 1; j < k; j++) {
            if (arr[i + j] > max) {
                max = arr[i + j];
            }
        }
        printf("%d ", max);
    }
    printf("\n");

    return 0;
}