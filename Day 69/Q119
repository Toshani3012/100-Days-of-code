#include <stdio.h>
#include <stdbool.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Boolean array to mark visited numbers
    bool visited[n+1];  
    for (int i = 0; i <= n; i++) {
        visited[i] = false;
    }

    // Single iteration to find repeated element
    for (int i = 0; i < n; i++) {
        if (visited[arr[i]]) {
            printf("Repeated element is: %d\n", arr[i]);
            return 0;
        }
        visited[arr[i]] = true;
    }

    printf("No repeated element found.\n");
    return 0;
}
