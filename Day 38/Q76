// Q76: Check if a matrix is symmetric

#include <stdio.h>

int main() {
    int n;
    printf("Enter size of square matrix (n x n): ");
    scanf("%d", &n);

    int mat[n][n], isSymmetric = 1;

    // Input matrix
    printf("Enter elements of the matrix:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &mat[i][j]);

    // Check symmetry
    for (int i = 0; i < n && isSymmetric; i++)
        for (int j = 0; j < n; j++)
            if (mat[i][j] != mat[j][i]) {
                isSymmetric = 0;
                break;
            }

    if (isSymmetric)
        printf("The matrix is symmetric.\n");
    else
        printf("The matrix is not symmetric.\n");

    return 0;
}
