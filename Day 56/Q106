// Q106 (Logic Enhancers)
// Program to find next greater element for each array element using brute-force (no stack)

#include <stdio.h>

int main() {
    int arr[100], size;

    // Input array size and elements
    printf("Enter size of array: ");
    scanf("%d", &size);
    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    // Find and print next greater elements
    for (int i = 0; i < size; i++) {
        int next = -1;
        for (int j = i + 1; j < size; j++) {
            if (arr[j] > arr[i]) {
                next = arr[j];
                break;
            }
        }
        printf("%d", next);
        if (i != size - 1) {
            printf(","); // comma-separated output
        }
    }
    printf("\n");

    return 0;
}