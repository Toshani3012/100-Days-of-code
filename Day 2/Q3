#include <stdio.h>

int main() {
    int num1, num2;

    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    if (num2 != 0) {
        printf("Remainder = %d\n", num1 % num2);
    } else {
        printf("Division by zero is not allowed.\n");
    }

    return 0;
}
