// Q108 (Logic Enhancers)
// Program to compute product of all elements except self for each index

#include <stdio.h>

int main() {
    int nums[100], answer[100], size;

    // Input array size and elements
    printf("Enter size of array: ");
    scanf("%d", &size);
    printf("Enter %d elements:\n", size);
    for (int i = 0; i < size; i++) {
        scanf("%d", &nums[i]);
    }

    // Step 1: Compute prefix products
    answer[0] = 1;
    for (int i = 1; i < size; i++) {
        answer[i] = answer[i - 1] * nums[i - 1];
    }

    // Step 2: Multiply with suffix products
    int suffix = 1;
    for (int i = size - 1; i >= 0; i--) {
        answer[i] *= suffix;
        suffix *= nums[i];
    }

    // Print result
    for (int i = 0; i < size; i++) {
        printf("%d", answer[i]);
        if (i != size - 1) {
            printf(","); // comma-separated output
        }
    }
    printf("\n");

    return 0;
}